{
  "schemaVersion": 1,
  "id": "ambiance",
  "version": "${version}",
  "name": "Ambiance!",
  "description": "A small Minecraft Mod that add new ambient effects to the Minecraft!",
  "authors": [
    "Team Chorus", "YeoXuHang"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/ambiance",
    "issues": "https://github.com/ChorusTeam/Ambiance/issues",
    "website": "https://patreon.com/chorus_team",
    "repo": "https://github.com/ChorusTeam/Ambiance"
  },
  "license": "Apache",
  "icon": "assets/ambiance/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.yeoxuhang.ambiance.client.AmbianceClient"
    ],
    "main": [
      "net.yeoxuhang.ambiance.Ambiance"
    ],
    "modmenu": [
      "net.yeoxuhang.ambiance.compat.AmbianceModMenuCompat"
    ]
  },

  "mixins": [
    "ambiance.mixins.json"
  ],
  "accessWidener": "ambiance.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "midnightlib": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "ambiance.discord": "https://discord.gg/PUJ42rkRBY",
        "ambiance.patreon": "https://patreon.com/chorus_team",
        "ambiance.source": "https://github.com/ChorusTeam/Ambiance"
      },
      "badges": [ "client" ]
    }
  }
}
